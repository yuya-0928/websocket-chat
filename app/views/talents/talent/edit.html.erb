<div class="talent-edit">
    <div>
        <%= form_with model: @talent, url: talents_talent_path(@talent.id), data: {remote: false} do |talent| %>
        <%= talent.label :name, 'タレント名' %>
        <%= talent.text_field :name %>
        <%= talent.label :email, 'email' %>
        <%= talent.text_field :email %>
        <%= talent.label :image, 'image' %>
        <%= talent.file_field :image %>
        <%= talent.submit '変更を保存' %>
        <% end %>
    </div>
</div>